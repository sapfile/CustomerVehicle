@model CustomerVehicle.Service.Application.Vehicle.Queries.GetAllVehicles.GetAllVehiclesModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <form id="radios">
        <input type="radio" value="1" name="parameter" checked="checked" /> All<br />
        <input type="radio" value="2" name="parameter" /> Age Between 20 and 30<br />
        <input type="radio" value="3" name="parameter" /> Before 1 January 2010<br />
        <input type="radio" value="4" name="parameter" /> Engine Size Over 1100
    </form>
    <div id="vehicleList">
        @Html.Partial("Partial/_VehicleList", Model)
    </div>

</div>

<script src="~/js/Vehicle/Index.js" type="text/javascript"></script>